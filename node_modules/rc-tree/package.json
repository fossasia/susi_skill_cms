{
  "name": "rc-tree",
  "version": "1.5.0",
  "description": "tree ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-tree",
    "tree"
  ],
  "main": "lib/index",
  "files": [
    "assets/*.css",
    "assets/*.png",
    "assets/*.gif",
    "dist",
    "lib"
  ],
  "entry": {
    "rc-tree": [
      "./assets/index.less",
      "./src/index.js"
    ]
  },
  "homepage": "http://github.com/react-component/tree",
  "author": "hualei5280@gmail.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/tree.git"
  },
  "bugs": {
    "url": "http://github.com/react-component/tree/issues"
  },
  "licenses": "MIT",
  "config": {
    "port": 8018
  },
  "scripts": {
    "build": "rc-tools run build",
    "dist": "rc-tools run dist",
    "compile": "rc-tools run compile",
    "gh-pages": "rc-tools run gh-pages",
    "start": "rc-tools run server",
    "pub": "rc-tools run pub",
    "lint": "rc-tools run lint",
    "karma": "rc-tools run karma",
    "saucelabs": "rc-tools run saucelabs",
    "test": "jest",
    "chrome-test": "rc-tools run chrome-test",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*"
    ]
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "css-animation": "^1.2.0",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.5",
    "es6-promise": "^3.1.2",
    "jest": "^18.0.0",
    "pre-commit": "1.x",
    "rc-dialog": "^6.1.0",
    "rc-tools": "^5.9.2",
    "rc-tooltip": "3.x",
    "rc-trigger": "^1.0.6",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "dependencies": {
    "classnames": "2.x",
    "object-assign": "4.x",
    "prop-types": "^15.5.8",
    "rc-animate": "2.x",
    "rc-util": "^4.0.2"
  },
  "pre-commit": [
    "lint"
  ]
}
